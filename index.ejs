<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Excel to HTML</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Login</th>
          <th>Reason Code</th>
          <th>Remarks</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(function(row) { %>
          <tr>
            <td><%= row.Login %></td>
            <td><%= row.ReasonCode %></td>
            <td><%= row.Remarks %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
